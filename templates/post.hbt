{{> header}}
    <div class="row">
      <div class="three columns offset-by-three">
          <h2>{{title}}</h2>
      </div>
      <div class="four columns">
          By {{author}} | Published {{moment publishDate format="M.D.YYYY"}}
          {{{contents}}}
      </div>  
      <hr class="row ten columns centered dotted"/>
    </div>
{{> footer}}