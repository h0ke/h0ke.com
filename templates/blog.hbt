{{> header}}
    <div class="content">
      <h2>weblog</h2>
      <ul>
        {{#each collections.posts}}
            <li>
                <a class="title" href="{{this.path}}">{{this.title}}</a>
                <article>{{{this.excerpt}}}</article>
            </li>
        {{/each}}
      </ul>
    </div>
{{> footer}}